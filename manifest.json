{
  "manifest_version": 3,
  "name": "Ocultar BairesDev en Computrabajo",
  "version": "1.0",
  "description": "Oculta ofertas de BairesDev LLC en Computrabajo.",
  "permissions": ["scripting"],
  "content_scripts": [
    {
      "matches": ["*://mx.computrabajo.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
